<!DOCTYPE html>
<html>
  <head>
    <title>ActivityFinder</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= yield %>
      <% flash.each do |type, msg| %>
    <div>
     <h2> <%= msg %> </h2>
    </div>
<% end %>
  </body>
</html>
